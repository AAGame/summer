#include <iostream>
#include <string.h>
using namespace std;
const int t = 1000;     //输入字符串最多t行;
int main()
{
    int N, i, j;
	cin>> N;
	char s[t][100];
	for(i = 0; i < N; i++){
	    scanf("%s", s[i]);
    }
	char ans_s[t][100];
	int sign[t] = {0};
	int ans_num[t][100] = {0};
	for(i = 0; i < N; i++){
		ans_s[i][0] = s[i][0];
		for(j = 1; j < strlen( s[i] ); j++){
			if(s[i][j] != s[i][j-1]){
				sign[i]++;
				ans_s[i][ sign[i] ] = s[i][j];
			}else{
				ans_num[i][ sign[i] ]++;
			}
		}
		for(j = 0; j <= sign[i]; j++){
			cout<< ans_s[i][j];
			if(ans_num[i][j] >= 1)
				cout<< ans_num[i][j] + 1;
		}
		cout<<endl;
	}
	return 0;
}
