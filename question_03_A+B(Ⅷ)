#include <iostream>
using namespace std;
const int N = 100;
const int M = 200;
int main()
{
    int T, i, j;
	cin>> T;
	int num[N];
	int con[N][M];
	int ans[N] = {0};
	for(i = 0; i < T; i++){
		cin>> num[i];
		for(j = 0; j < num[i]; j++){
			cin>> con[i][j];
			ans[i] += con[i][j];
		}
	}
	for(i = 0; i < T; i++){
		cout<< ans[i]<<endl<<endl;
	}
	system("pause");
	return 0;
}
