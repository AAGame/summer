#include <iostream>
using namespace std;
void b_s(int n, int a[][7][480], int i, int k)
{
	int j = 0;
	do{
		a[i][j++][k] = n % 2;
	}while(n = n / 2);
}
int main()
{
    int T, i, j, k;
	cin>> T;
	int C[20];
	int in[20][80][5];
	int ans[20][7][480] = {0};
	for(i = 0; i < T; i++){
		cin>> C[i];
		for(j = 0; j < C[i]; j++){
			for(k = 0; k < 5; k++){
				cin>> hex>> in[i][j][k];
				b_s(in[i][j][k], ans, i, j*6+k);
			}
		}
	}
	for(i = 0; i < T; i++){
		cout<<"Case "<<i + 1<<":"<<endl<<endl;
		for(j = 0; j < 7; j++){
			for(k = 0; k < 6*C[i]-1; k++){
				if(ans[i][j][k] == 0)
					cout<< " ";
				else
					cout<<"*";
			}
			cout<<endl;
		}
	}
	system("pause");
	return 0;
}
